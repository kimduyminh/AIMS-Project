<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AIMS.hust.soict.cybersec.aims.screen.controller.ViewStoreController">
    <children>
        <HBox prefHeight="38.0" prefWidth="200.0">
            <children>
                <Label text="AIMS" textFill="#004cff">
                    <font>
                        <Font size="50.0" />
                    </font>
                    <padding>
                        <Insets left="10.0" />
                    </padding></Label>
                <Region prefHeight="38.0" prefWidth="84.0" HBox.hgrow="ALWAYS" />
                <Button mnemonicParsing="false" onAction="#btnViewCartPressed" prefHeight="50.0" prefWidth="100.0" text="View Cart">
                    <HBox.margin>
                        <Insets right="20.0" />
                    </HBox.margin></Button>
            </children>
        </HBox>
        <ScrollPane prefHeight="760.0" prefWidth="1024.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <GridPane fx:id="gridPane" prefHeight="0.0" prefWidth="0.0">
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                        </GridPane>
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
    </children>
</VBox>